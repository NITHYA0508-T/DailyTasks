class Person
{
    String name;
    int age;
    public Person(String name,int age)
    {
        this.name=name;
        this.age=age;
    }
    public void display()
    {
        System.out.println("Name:"+name);
        System.out.println("Age:"+age);
        System.out.println("My name is"+name+"My age is"+age+"in college");
    }
    public static void main (String[] args)
    {
        Person a=new Person("NITHYA",18);
        a.display();
    }
    }
